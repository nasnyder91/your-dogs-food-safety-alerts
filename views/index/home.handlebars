<div class="row">
  {{#each goodFoods}}
    <div class="col m3 foodContainer">
      <div class="card green foodCard">
        <div class="cardFront">
          <div class="card-image">

          </div>
          <div class="card-content">
            <span class="card-title">{{name}}</span>
          </div>
        </div>
        <div class="card-content cardBack" style="display: none;">
          <p>{{description}}</p>
        </div>
      </div>
    </div>
  {{/each}}
</div>
